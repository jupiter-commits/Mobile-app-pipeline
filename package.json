{
  "name": "Wellbeing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "uAll": "./gradlew uninstallAll",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "lint": "eslint .",
    "start": "react-native start",
    "a:d:d": "react-native run-android --mode=developmentDebug --appId com.wellbeing.development",
    "a:s:d": "react-native run-android --mode=stagingDebug --appId com.wellbeing.staging",
    "a:p:d": "react-native run-android --mode=productionDebug --appId com.wellbeing",
    "a:d:r": "react-native run-android --mode=developmentRelease --appId com.wellbeing.development",
    "a:s:r": "react-native run-android --mode=stagingRelease --appId com.wellbeing.staging",
    "a:p:r": "react-native run-android --mode=productionRelease --appId com.wellbeing",
    "i:d:d": "react-native run-ios --scheme Wellbeing-development --mode Debug",
    "i:s:d": "react-native run-ios --scheme Wellbeing-staging --mode Debug",
    "i:p:d": "react-native run-ios --scheme Wellbeing-production --mode Debug",
    "i:d:r": "cd ios && xcodebuild -scheme Wellbeing-development build",
    "i:s:r": "cd ios && xcodebuild -scheme Wellbeing-staging build",
    "i:p:r": "cd ios && xcodebuild -scheme Wellbeing-production build",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@shopify/restyle": "^2.4.4",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-bootsplash": "^5.5.3",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "^3.12.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
