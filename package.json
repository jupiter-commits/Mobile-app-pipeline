{
  "name": "Wellbeing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "uAll": "./gradlew uninstallAll",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "lint": "eslint .",
    "envReset": "rm -f node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m node_modules/react-native-config/android/build/generated/source/buildConfig/debug/com/lugg/RNCConfig/BuildConfig.java node_modules/react-native-config/android/build/generated/source/buildConfig/release/com/lugg/RNCConfig/BuildConfig.java",
    "start": "react-native start",
    "a:d:d": "react-native run-android --mode=developmentDebug --appId com.wellbeing.development",
    "a:s:d": "react-native run-android --mode=stagingDebug --appId com.wellbeing.staging",
    "a:p:d": "react-native run-android --mode=productionDebug --appId com.wellbeing",
    "a:d:r": "react-native run-android --mode=developmentRelease --appId com.wellbeing.development",
    "a:s:r": "react-native run-android --mode=stagingRelease --appId com.wellbeing.staging",
    "a:p:r": "react-native run-android --mode=productionRelease --appId com.wellbeing",
    "i:d:d": "react-native run-ios --scheme Wellbeing-development --mode Debug",
    "i:s:d": "react-native run-ios --scheme Wellbeing-staging --mode Debug",
    "i:p:d": "react-native run-ios --scheme Wellbeing-production --mode Debug",
    "i:d:r": "cd ios && xcodebuild -scheme Wellbeing-development build",
    "i:s:r": "cd ios && xcodebuild -scheme Wellbeing-staging build",
    "i:p:r": "cd ios && xcodebuild -scheme Wellbeing-production build",
    "test": "jest"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@invertase/react-native-apple-authentication": "^2.3.0",
    "@marceloterreiro/flash-calendar": "^1.1.0",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/auth": "^20.1.0",
    "@react-native-firebase/database": "^21.5.0",
    "@react-native-firebase/firestore": "^20.1.0",
    "@react-native-firebase/storage": "^21.6.0",
    "@react-native-google-signin/google-signin": "latest",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@shopify/flash-list": "^1.7.1",
    "@shopify/react-native-skia": "^1.3.6",
    "@shopify/restyle": "^2.4.4",
    "apisauce": "^3.1.0",
    "buffer": "^6.0.3",
    "event-target-shim": "6.0.2",
    "i18next": "^23.11.5",
    "intl-pluralrules": "^2.0.1",
    "lottie-react-native": "^7.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^14.1.2",
    "react-native": "0.75.3",
    "react-native-bootsplash": "^6.3.2",
    "react-native-calendar-strip": "^2.2.6",
    "react-native-config": "^1.5.3",
    "react-native-date-picker": "^5.0.7",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-image-picker": "^7.1.2",
    "react-native-incall-manager": "^4.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-live-transcription": "git@github.com:abumubaarak/react-native-live-transcription.git",
    "react-native-mmkv": "^3.0.2",
    "react-native-pager-view": "^6.5.1",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.15.3",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "^15.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-webrtc": "^124.0.4",
    "react-navigation-heavy-screen": "^1.2.1",
    "socket.io-client": "^4.8.1"
